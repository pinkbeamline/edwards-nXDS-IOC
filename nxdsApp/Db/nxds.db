record (stringin, "$(BL):$(DEV):ID")
{
    field (DTYP, "stream")
    field (INP,  "@nxds.proto getID $(PORT)")
    field (SCAN, "Passive")
}

record (ai, "$(BL):$(DEV):Speed")
{
    field (DTYP, "stream")
    field (INP,  "@nxds.proto getSpeed $(PORT)")
    field (SCAN, "2 second")
    field (PREC, "0")
    field (EGU, "RPM")
}

record (calc, "$(BL):$(DEV):Speedpc")
{
    field (INPA, "$(BL):$(DEV):Speed CPP")
    field (CALC, "(A/30)*100")
    field (PREC, "1")
    field (EGU, "%")
}

